START0:
	MVI A,10H
    	STA 0B00H
    	STA 0B01H
    	STA 0B02H
    	STA 0B03H
    	STA 0B04H
    	STA 0B05H
START:
	CALL KIND 
	CPI 00H
	JC START
	CPI 0AH
	JNC START
	STA 0B05H
	MOV B,A
	LXI D,0B00H
	CALL STDM
	CALL DCD
START1:
	CALL KIND 
	CPI 00H
	JC START1
	CPI 0AH
	JNC START1
	STA 0B04H
	LXI D,0B00H
	CALL STDM
	CALL DCD
	LDA 0B05H
	MOV B,A
	LDA 0B04H
	ADD B
	CPI 10H
	JC NOT_OVER
	MVI A,11H
NOT_OVER:
	STA 0B00H
	LXI D,0B00H
	CALL STDM
	CALL DCD
	JMP START0

END